{
  "name": "chill-rest-api",
  "version": "1.1.1",
  "description": "REST API for the chill monitoring service.",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "start": "nodemon --watch src/ --exec babel-node src/ --source-maps",
    "clean": "rimraf dist/",
    "babel": "babel src/ -d dist/",
    "build": "run-s lint clean babel",
    "make:migration": "knex migrate:make",
    "prettify": "prettier --write src/**/*.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "synchronize": "babel-node ./src/scripts/synchronize"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "chill",
    "rest-api"
  ],
  "authors": [],
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "bookshelf-camelcase": "^1.1.4",
    "boom": "^4.2.0",
    "camelize": "^1.0.0",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "express": "^4.14.1",
    "helmet": "^3.2.0",
    "http-status-codes": "^1.0.6",
    "joi": "^10.1.0",
    "knex": "^0.19.5",
    "lodash": "^4.17.21",
    "memory-cache": "^0.1.6",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.0.2",
    "pg": "^7.1.2",
    "serve-favicon": "^2.4.2",
    "sqlite3": "^4.0.6",
    "swagger-jsdoc": "^3.2.6",
    "webpack-merge": "^4.1.0",
    "winston": "^2.3.0",
    "winston-daily-rotate-file": "^1.4.0",
    "yamljs": "^0.2.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "eslint": "^5.15.3",
    "eslint-config-leapfrog": "^2.0.0-beta.0",
    "eslint-config-prettier": "^4.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.1",
    "semver": "^5.3.0"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.10.8"
  }
}
